using System;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Csharp_Lite
{
    class Program
    {
        static void Main(string[] args)
        {
           for(int i = 0; i<= 100; i++)
            {
             if( i % 7 == 0)
                {
                    Console.WriteLine(i);
                }
            }
        }
    }
}
