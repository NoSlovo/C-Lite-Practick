using System;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Csharp_Lite
{
    class Program
    {
        static void Main(string[] args)
        {
            int step;
            int end = 98;
            for (step = 7;step <= end ;step+=7)
            {
                Console.WriteLine(step);
            }
        }
    }
}
