using System;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Csharp_Lite
{
    class Program
    {
        static void Main(string[] args)
        {
            int step = 7;
            int end = 98;

            for(int i = step; i <= end;i+= step)
            {
                Console.WriteLine(i);
            }
        }
    }
}
